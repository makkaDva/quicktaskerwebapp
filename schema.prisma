// prisma/schema.prisma
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql" // or "mysql", "sqlite", etc.
  url      = env("DATABASE_URL")
}

// Define your database models here
model User {
  id    Int    @id @default(autoincrement())
  name  String
  email String @unique
}